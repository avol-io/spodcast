{
  "projectType": "library",
  "sourceRoot": "libs/player/src",
  "prefix": "spoticast",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/player"
      ],
      "options": {
        "jestConfig": "libs/player/jest.config.ts",
        "passWithNoTests": true
